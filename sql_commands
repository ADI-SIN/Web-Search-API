CREATE TABLE items (
    name VARCHAR(255),
    description VARCHAR(255),
    status INT,
   	type INT
);

CREATE TABLE item_status (
    status_type INT,
    status_name VARCHAR(255)
);

CREATE TABLE item_type (
    type INT,
    name VARCHAR(255)
);

